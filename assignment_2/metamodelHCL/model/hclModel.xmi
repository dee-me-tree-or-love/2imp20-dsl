<?xml version="1.0" encoding="UTF-8"?>
<metamodelHCL:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:metamodelHCL="http://www.example.org/metamodelHCL"
    xsi:schemaLocation="http://www.example.org/metamodelHCL metamodelHCL.ecore">
  <computer
      label="my_computer1">
    <component
        xsi:type="metamodelHCL:Processing"
        label="my_CPU1">
      <processingproperty
          xsi:type="metamodelHCL:Cores"
          number="4"/>
      <processingproperty
          xsi:type="metamodelHCL:Speed"
          speed="3.1"/>
      <processingproperty
          xsi:type="metamodelHCL:L1"
          number="16"
          type="KiB"/>
    </component>
    <component
        xsi:type="metamodelHCL:Processing"
        label="my_CPU2">
      <processingproperty
          xsi:type="metamodelHCL:Cores"
          number="6"/>
      <processingproperty
          xsi:type="metamodelHCL:Speed"
          speed="2.7"/>
      <processingproperty
          xsi:type="metamodelHCL:L1"
          number="16"
          type="KiB"/>
    </component>
    <component
        xsi:type="metamodelHCL:Display"
        label="my_Display1">
      <displayproperty
          xsi:type="metamodelHCL:Diagonal"
          inch="30"/>
      <displayproperty
          xsi:type="metamodelHCL:Type"
          dtype="4K"/>
    </component>
    <component
        xsi:type="metamodelHCL:Display"
        label="my_Display2">
      <displayproperty
          xsi:type="metamodelHCL:Diagonal"
          inch="40"/>
      <displayproperty
          xsi:type="metamodelHCL:Type"
          dtype="4K"/>
    </component>
  </computer>
  <computer
      label="my_computer2">
    <component
        xsi:type="metamodelHCL:Processing"
        label="my_CPU3">
      <processingproperty
          xsi:type="metamodelHCL:Cores"
          number="6"/>
      <processingproperty
          xsi:type="metamodelHCL:Speed"
          speed="4.2"/>
      <processingproperty
          xsi:type="metamodelHCL:L1"
          number="16"
          type="KiB"/>
    </component>
    <component
        xsi:type="metamodelHCL:Processing"
        label="my_CPU4">
      <processingproperty
          xsi:type="metamodelHCL:Cores"
          number="8"/>
      <processingproperty
          xsi:type="metamodelHCL:Speed"
          speed="3.3"/>
      <processingproperty
          xsi:type="metamodelHCL:L1"
          number="16"
          type="KiB"/>
    </component>
    <component
        xsi:type="metamodelHCL:Display"
        label="my_Display3">
      <displayproperty
          xsi:type="metamodelHCL:Diagonal"
          inch="40"/>
      <displayproperty
          xsi:type="metamodelHCL:Type"
          dtype="Full-HD"/>
    </component>
    <component
        xsi:type="metamodelHCL:Display"
        label="my_Display4">
      <displayproperty
          xsi:type="metamodelHCL:Diagonal"
          inch="30"/>
      <displayproperty
          xsi:type="metamodelHCL:Type"
          dtype="5K"/>
    </component>
  </computer>
</metamodelHCL:Model>
