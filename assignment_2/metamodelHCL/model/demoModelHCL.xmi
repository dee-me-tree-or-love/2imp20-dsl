<?xml version="1.0" encoding="UTF-8"?>
<metamodelHCL:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:metamodelHCL="http://www.example.org/metamodelHCL"
    xsi:schemaLocation="http://www.example.org/metamodelHCL metamodelHCL.ecore">
  <computers
      label="my_computer">
    <components
        xsi:type="metamodelHCL:Processing"
        label="my_cpu1">
      <properties
          xsi:type="metamodelHCL:L1CacheProperty"
          size="32"
          unit="KiB"/>
      <properties
          xsi:type="metamodelHCL:L2CacheProperty"
          size="128"
          unit="KiB"/>
      <properties
          xsi:type="metamodelHCL:CoresProperty"
          number="2"/>
      <properties
          xsi:type="metamodelHCL:SpeedProperty"
          speed="2.34"
          unit="GHz"/>
    </components>
    <components
        xsi:type="metamodelHCL:Display"
        label="my_display">
      <properties
          xsi:type="metamodelHCL:DiagonalProperty"
          size="5"
          unit="Inch"/>
      <properties
          xsi:type="metamodelHCL:ResolutionProperty"
          type="5K"/>
    </components>
    <components
        xsi:type="metamodelHCL:Display"
        label="my_display2">
      <properties
          xsi:type="metamodelHCL:DiagonalProperty"
          size="5"
          unit="Inch"/>
      <properties
          xsi:type="metamodelHCL:ResolutionProperty"
          type="4K"/>
    </components>
    <components
        xsi:type="metamodelHCL:Processing"
        label="my_cpu2">
      <properties
          xsi:type="metamodelHCL:SpeedProperty"
          speed="3.2"
          unit="GHz"/>
      <properties
          xsi:type="metamodelHCL:L1CacheProperty"
          size="24"
          unit="MiB"/>
      <properties
          xsi:type="metamodelHCL:CoresProperty"
          number="4"/>
    </components>
  </computers>
  <computers
      label="computer_2 ">
    <components
        xsi:type="metamodelHCL:Processing"
        label="ryzen 5">
      <properties
          xsi:type="metamodelHCL:CoresProperty"
          number="6"/>
      <properties
          xsi:type="metamodelHCL:SpeedProperty"
          speed="3.8"
          unit="GHz"/>
      <properties
          xsi:type="metamodelHCL:L1CacheProperty"
          size="32"
          unit="KiB"/>
    </components>
    <components
        xsi:type="metamodelHCL:Display"
        label="benq">
      <properties
          xsi:type="metamodelHCL:DiagonalProperty"
          size="27"
          unit="inch"/>
      <properties
          xsi:type="metamodelHCL:ResolutionProperty"
          type="5K"/>
    </components>
  </computers>
</metamodelHCL:Model>
